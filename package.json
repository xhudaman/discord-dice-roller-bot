{
  "name": "tymora",
  "version": "1.0.0",
  "description": "Discord dice rolling bot created for my discord community",
  "main": "index.mjs",
  "type": "module",
  "repository": "git@github.com:xhudaman/discord-dice-roller-bot.git",
  "author": "Matthew Edell",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "discord.js": "^12.5.1"
  },
  "devDependencies": {
    "jest": "^27.2.5",
