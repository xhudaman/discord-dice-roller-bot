{
  "name": "tymora",
  "version": "1.0.0",
  "description": "Discord dice rolling bot created for my discord community",
  "main": "index.mjs",
  "type": "module",
  "repository": "git@github.com:xhudaman/discord-dice-roller-bot.git",
  "author": "Matthew Edell",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-jest": "^27.2.5",
    "jest": "^29.4.1",
    "nodemon": "^2.0.6"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|mjs)$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "start": "nodemon index.mjs",
    "test": "jest",
    "ci": "pnpm i --frozen-lockfile && CI=true pnpm test"
  }
}
